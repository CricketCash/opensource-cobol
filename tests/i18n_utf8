      $at_traceoff
      $at_times_p && times >$at_times_file
    ) 5>&1 2>&1 | eval $at_tee_pipe
    at_status=`cat $at_status_file`
    ;;

  6 ) # 6. user-defined-word.at:99: Long section name
    at_setup_line='user-defined-word.at:99'
